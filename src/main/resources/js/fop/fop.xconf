<?xml version="1.0" encoding="UTF-8"?>
<fop version="2.1">
	<!-- cache for discovered fonts; works in conjunction with auto-detect -->
	<!-- if set to false auto-detect is executed every time slowing PDF generation --> 
	<use-cache>true</use-cache>
	
	<!-- only PDF renderer is actually used -->
	<renderers>
		<renderer mime="application/pdf">
			<fonts>
				<auto-detect />
			</fonts>
		</renderer>
	</renderers>
</fop>
